    /* CURSOR ANIMATION */
    .cursor {
      position: fixed;
      width: 3.125rem;
      height: 3.125rem;
      border: 0.0625rem solid #c6c6c6;
      border-radius: 50%;
      left: 0;
      top: 0;
      pointer-events: none;
      transform: translate(-50%, -50%);
      transition: 0.15s;
      z-index: 15;
      mix-blend-mode: difference;
    }

    .cursor2 {
      position: fixed;
      width: 0.625rem;
      height: 0.625rem;
      background-color: #c6c6c6;
      border-radius: 50%;
      left: 0;
      top: 0;
      pointer-events: none;
      transform: translate(-50%, -50%);
      transition: 0.2s;
      z-index: 15;
      mix-blend-mode: difference;
    }

    .wrapper .content:hover ~ .cursor2.main {
      transform: scale(9);
    }

    .cursor.main {
      position: fixed;
      width: 2.125rem;
      height: 2.125rem;
      border: 0.0625rem solid #ff5858;
      border-radius: 50%;
      left: 0;
      top: 0;
      pointer-events: none;
      transform: translate(-50%, -50%);
      transition: 0.2s;
    }

    .cursor2.main {
      position: fixed;
      width: 1.625rem;
      height: 1.625rem;
      background: #ff5858;
      border-radius: 50%;
      left: 0;
      top: 0;
      pointer-events: none;
      transform: translate(-50%, -50%);
      transition: 0.25s;
      mix-blend-mode: difference;
    }

    .header-text h1:hover ~ .cursor2.main {
      transform: scale(6);
    }

    .loading-text {
      width: 6.25rem;
      height: 6.25rem;
      position: absolute;
      font-size: 3.75rem;
